html
    head
        title Temperature Monitor - Log
        include meta.pug
    body(class="w3-theme-d1")
        include navbar.pug
        div(class="w3-container w3-center w3-margin-right w3-margin-left w3-theme-l1 w3-card-4 w3-round")
            h1 Temperature log
            div(class="w3-padding w3-section scroll")
                ul(class="w3-ul")
                    each element in readings
                        li(class="w3-right-align")= element